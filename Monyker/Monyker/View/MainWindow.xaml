<Window x:Class="Monyker.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Model="clr-namespace:Monyker.Model"
        xmlns:View="clr-namespace:Monyker.View"
        DataContext="{StaticResource MainViewModel}"
        Title="MainWindow" Height="500" Width="500">
    <Grid Margin="12">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="4*"/>
        </Grid.RowDefinitions>
        <View:AccountListUserControl Grid.Row="2"/>
        <!--View:TransactionUserControl Grid.Column="2" Grid.Row="2"/-->
    </Grid>
</Window>
